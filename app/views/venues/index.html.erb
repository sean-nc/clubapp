<p id="notice"><%= notice %></p>

<h1>Venues</h1>
<% @venues.each do |venue| %>
  <ul>
    <h4><%= venue.name %></h4>

    <%= link_to 'Show', venue %>
    <%= link_to 'Edit', edit_venue_path(venue) %>
    <%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %>
  </ul>

  <br>
  <br>

<% end %>

